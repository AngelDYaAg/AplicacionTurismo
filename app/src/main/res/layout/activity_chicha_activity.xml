<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#000000"
    tools:context=".chicha_activity">

    <Button
        android:id="@+id/btningrediente3"
        android:layout_width="302dp"
        android:layout_height="63dp"
        android:layout_marginTop="468dp"
        android:background="@drawable/ingredientes"
        android:onClick="onclick_chicha_ingre"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnvalornutrional3"
        android:layout_width="302dp"
        android:layout_height="63dp"
        android:background="@drawable/valor_nutricional"
        android:onClick="onclick_chicha_valor"
        app:layout_constraintEnd_toEndOf="@+id/btningrediente3"
        app:layout_constraintStart_toStartOf="@+id/btningrediente3"
        app:layout_constraintTop_toBottomOf="@+id/btningrediente3" />

    <Button
        android:id="@+id/btncomollegar3"
        android:layout_width="302dp"
        android:layout_height="63dp"
        android:background="@drawable/como_llegar"
        app:layout_constraintEnd_toEndOf="@+id/btnvalornutrional3"
        app:layout_constraintStart_toStartOf="@+id/btnvalornutrional3"
        app:layout_constraintTop_toBottomOf="@+id/btnvalornutrional3" />

    <ImageView
        android:id="@+id/imageView34"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/chicha" />
</androidx.constraintlayout.widget.ConstraintLayout>