<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#000000">

    <ImageView
        android:id="@+id/imageView31"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/fritada_de_llama" />

    <Button
        android:id="@+id/btnIngredientes"
        android:layout_width="302dp"
        android:layout_height="63dp"
        android:layout_marginTop="464dp"
        android:background="@drawable/ingredientes"
        android:onClick="onclick_proteina_ingredientes"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView31" />

    <Button
        android:id="@+id/btnValorNutricional"
        android:layout_width="302dp"
        android:layout_height="63dp"
        android:background="@drawable/valor_nutricional"
        android:onClick="onclick_proteina_valor_nutricional"
        app:layout_constraintEnd_toEndOf="@+id/btnIngredientes"
        app:layout_constraintStart_toStartOf="@+id/btnIngredientes"
        app:layout_constraintTop_toBottomOf="@+id/btnIngredientes" />

    <Button
        android:id="@+id/btnComoLlegar"
        android:layout_width="302dp"
        android:layout_height="63dp"
        android:layout_marginEnd="24dp"
        android:background="@drawable/como_llegar"
        app:layout_constraintEnd_toEndOf="@+id/btnValorNutricional"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/btnValorNutricional"
        app:layout_constraintTop_toBottomOf="@+id/btnValorNutricional" />

</androidx.constraintlayout.widget.ConstraintLayout>