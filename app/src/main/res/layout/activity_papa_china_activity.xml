<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#000000"
    tools:context=".papa_china_activity">

    <Button
        android:id="@+id/btningrediente5"
        android:layout_width="302dp"
        android:layout_height="63dp"
        android:layout_marginTop="468dp"
        android:background="@drawable/ingredientes"
        android:onClick="onclick_papachina_ingre"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnvalornutrional5"
        android:layout_width="302dp"
        android:layout_height="63dp"
        android:background="@drawable/valor_nutricional"
        android:onClick="onclick_papachina_valor"
        app:layout_constraintEnd_toEndOf="@+id/btningrediente5"
        app:layout_constraintStart_toStartOf="@+id/btningrediente5"
        app:layout_constraintTop_toBottomOf="@+id/btningrediente5" />

    <Button
        android:id="@+id/btncomollegar5"
        android:layout_width="302dp"
        android:layout_height="63dp"
        android:background="@drawable/como_llegar"
        app:layout_constraintEnd_toEndOf="@+id/btnvalornutrional5"
        app:layout_constraintStart_toStartOf="@+id/btnvalornutrional5"
        app:layout_constraintTop_toBottomOf="@+id/btnvalornutrional5" />

    <ImageView
        android:id="@+id/imageView37"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/papa_china" />
</androidx.constraintlayout.widget.ConstraintLayout>